<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk+" version="4.0"/>

    <object class="GtkBox" id="root">
        <property name="name">main_view</property>
        <property name="visible">True</property>
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <property name="orientation">horizontal</property>

        <child>
            <object class="GtkBox" id="activity_pane">
                <property name="visible">True</property>
                <property name="orientation">vertical</property>

                <child>
                    <object class="GtkLabel" id="smudge">
                        <property name="name">smudge</property>
                        <property name="visible">True</property>
                        <property name="label">00:00</property>
                    </object>
                </child>

                <child>
                    <object class="GtkLabel" id="obombo">
                        <property name="name">obombo</property>
                        <property name="visible">True</property>
                        <property name="label">NONE</property>
                    </object>
                </child>

                <child>
                    <object class="GtkLabel" id="bps">
                        <property name="name">bps</property>
                        <property name="visible">True</property>
                        <property name="label">0.00 BPS</property>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
